@using Sitecore.Mvc
@using Sitecore.Mvc.Analytics;
@using Sitecore.Mvc.Analytics.Extensions
@using Sitecore.Mvc.Presentation

<!DOCTYPE html>
<html lang="en-gb">
<head>
    <title>@Html.Sitecore().Field("title", new { DisableWebEdit = true })</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    @Html.Sitecore().VisitorIdentification()    
    <link href="~/dist/css/app.css" media="screen" rel="stylesheet" />
</head>
<body>
    
    @RenderBody()
    
<!-- scripts -->
    <script src="~/Scripts/jquery-1.12.4.min.js"></script>
    <script src="~/Scripts/Modules/tracking.js"></script>
    <script src="~/Scripts/Modules/gallery.js"></script>
    <script src="~/Scripts/app.js"></script>

